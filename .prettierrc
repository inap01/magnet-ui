{
    "printWidth": 80,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "preserve",
    "trailingComma": "none",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "proseWrap": "never",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf",
    "embeddedLanguageFormatting": "auto"
}