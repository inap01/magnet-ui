    "@typescript-eslint/parser": "4.16.1",
    "babel-loader": "8.2.2",
    "babel-preset-react-app": "10.0.0",
    "eslint": "7.32.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-storybook": "0.6.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "1.39.0",
    "sass-loader": "10.1.1",
    "typescript": "4.2.2"
  }
